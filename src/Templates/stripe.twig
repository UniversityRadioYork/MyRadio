{% extends 'base.twig' %}
{% block content %}
<header class="page-header">
  <h1>{{ title }}{% if subtitle %} <small>{{subtitle}}</small>{% endif %}</h1>
</header>
<div id="grid" class="transBG clearfix">
  <div id="content-body" class="clearfix">
    {% if submenu|length > 0 %}
    {% include 'parts/submenu.twig' %}
    {% endif %}
  {% block stripecontent %}{{stripedata | raw}}{% endblock %}
      </div>
</div>
{% endblock %}
